    <Item>
      <New class="org.mortbay.jetty.nio.SelectChannelConnector">
        <Set name="host">172.16.1.214</Set>
        <Set name="port"><SystemProperty name="jetty.port" default="18080"/></Set>
      </New>
    </Item>
  </Array>
</Set>

</Configure>
